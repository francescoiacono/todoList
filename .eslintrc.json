{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "next/core-web-vitals"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "rules":{
    "no-console": "warn",
    "@typescript-eslint/no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ]
  }
}
